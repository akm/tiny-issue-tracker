{
	"latest": {
		"version": "0.0.3",
		"docker-images": [
			{
				"name": "tiny-issue-tracker",
				"tag": "0.0.3",
				"archive": "https://storage.googleapis.com/ttd-test-public1/tiny-issue-tracker/0.0.3/tiny-issue-tracker-0.0.3-aarch64.tar.gz"
			}
		],
		"docker-compose": {
			"file": "https://akm.github.io/tiny-issue-tracker/production/0.0.3/docker-compose-aarch64.yml"
		},
		"envs": [
			{ "name": "APP_NAME", "value": "tiny-issue-tracker" },
			{ "name": "APP_VERSION", "value": "0.0.3" },
			{
				"name": "MYSQL_ROOT_PASSWORD",
				"type": "MysqlPassword",
				"args": { "length": 16 }
			},
			{
				"name": "MYSQL_PASSWORD",
				"type": "MysqlPassword",
				"args": { "length": 16 }
			},
			{ "name": "MYSQL_DATABASE", "value": "tiny_issue_tracker" },
			{
				"name": "GOOGLE_AUTH_CALLBACK_ORIGIN",
				"value": "http://localhost:5173"
			},
			{ "name": "GOOGLE_CLIENT_ID", "type": "HostEnv" },
			{ "name": "GOOGLE_CLIENT_SECRET", "type": "HostEnv" }
		]
	}
}
